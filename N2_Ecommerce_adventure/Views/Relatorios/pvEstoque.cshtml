@model List<EstoqueModel>
{
@if (Model != null)
{
    <table>
        <tr>
            <th>Nome</th>
            <th>Quantidade Total</th>
            <th>Quantidade em ordem</th>
        </tr>

        @foreach (EstoqueModel e in Model)
        {
            <tr>
                <td>@e.Nome</td>
                <td>@e.Quantidade</td>
                <td>@e.QuantidadeEmOrdem.</td>
            </tr>
        }

    </table>
}

}
